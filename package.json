{
  "name": "sudoku-toolbox",
  "version": "1.1.3",
  "description": "Simple sudoku toolbox",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier ./.prettierrc.json --write ./src",
    "postbuild": "rimraf dist/types && ncp src/types dist/types types && copy package.json dist && copy README.md dist && copy LICENSE dist",
    "npm-publish": "yarn build && npm version patch && npm publish ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shiyan7/sudoku-toolbox.git"
  },
  "keywords": [
    "generator",
    "solver",
    "areas",
    "sudoku"
  ],
  "author": "Shiyan7",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shiyan7/sudoku-toolbox/issues"
  },
  "homepage": "https://github.com/Shiyan7/sudoku-toolbox#readme",
  "devDependencies": {
    "ncp": "^2.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "sudoku-gen": "^1.0.2"
  }
}
